{
    "sourceMaps": false,
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": ["Chrome >= 52", "FireFox >= 44", "Safari >= 7", "Explorer 11", "last 4 Edge versions"]
                },
                "useBuiltIns": true,
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ]
    ]
}
